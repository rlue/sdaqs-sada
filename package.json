{
  "name": "sdaqs",
  "version": "0.0.0",
  "description": "Source-Differentiated Air Quality System",
  "main": "app/index.js",
  "author": "Ryan Lue <hello@ryanlue.com>",
  "private": true,
  "scripts": {
    "build": "NODE_ENV='production' webpack --config config/webpack.js"
  },
  "installConfig": {
    "pnp": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "prettier",
      "react"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "semi": false,
          "trailingComma": "all",
          "tabWidth": 2,
          "arrowParens": "always"
        }
      ],
      "react/jsx-props-no-spreading": "off",
      "react/require-default-props": [
        "error",
        {
          "ignoreFunctionalComponents": true
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "assets-webpack-plugin": "^3.9.10",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "latest",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yarn": "^2.0.0-rc.27"
  },
  "dependencies": {
    "antd": "^4.0.2",
    "downshift": "^5.0.3",
    "fuse.js": "^3.4.6",
    "lodash.debounce": "^4.0.8",
    "mapbox-gl": "^1.8.0",
    "pnp-webpack-plugin": "^1.6.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.9.0"
  }
}
